@model AppUser

@{
    int count = 0;
}

@if (ViewBag.Message != null)
{
    <script type="text/javascript">
        window.onload = function () {
            alert("@ViewBag.Message");
        }
    </script>
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width,initial-scale=1" name="viewport">
    <meta content="description" name="description">
    <meta name="google" content="notranslate" />

    <!-- Disable tap highlight on IE -->
    <meta name="msapplication-tap-highlight" content="no">

    <link rel="apple-touch-icon" sizes="180x180" href="../wwwroot/assets/apple-icon-180x180.png">
    <link href="./assets/favicon.ico" rel="icon">

    <title>Lloyd's Portfolio</title>

    <link href="./css/main.d8e0d294.css" rel="stylesheet">
</head>

<body class="">

    <!-- Add your content of header -->
    <div class="background-color-layer" style="background-image: url('assets/images/image-01.jpg'); background-position:right; background-position:center"></div>
    <main class="content-wrapper">
        <header class="white-text-container section-container">
            <div class="text-center">
                <h1>I am @Model.FirstName</h1>
                <p>@Model.JobTitle</p>
                <p>
                    @*<a class="fa-icon fa-icon-2x" href="https://facebook.com/" title="">
                            <i class="fa fa-facebook"></i>
                        </a>*@
                    <a class="fa-icon fa-icon-2x" href="https://twitter.com/" title="">
                        <i class="fa fa-twitter"></i>
                    </a>
                    @*<a class="fa-icon fa-icon-2x" href="https://dribbble.com/" title="">
                            <i class="fa fa-dribbble"></i>
                        </a>*@
                    <a class="fa-icon fa-icon-2x" href="https://www.linkedin.com/in/eugene-lloyduche-8a12a1192/" title="">
                        <i class="fa fa-linkedin"></i>
                    </a>
                    <a class="fa-icon fa-icon-2x" href="https://vimeo.com/" title="">
                        <i class="fa fa-vimeo"></i>
                    </a>
                </p>
            </div>
        </header>



        <!-- Add your site or app content here -->

        <div class="container">
            <div class="row">
                <div class="col-xs-12">

                    <div class="card">
                        <div class="card-block">
                            <h2>About me</h2>
                            <div class="row">
                                <div class="col-md-4">
                                    <p><img src="~/assets/@Model.PhotoUrl" class="img-responsive img-rounded" alt=""></p>
                                </div>
                                <div class="col-md-8">

                                    <p>
                                        A self-motivated individual with excellent communication
                                        skills. Passionate software developer who seeks to have
                                        the required knowledge to get the work done efficiently
                                        and effectively.

                                    </p>
                                    <p>
                                        I have a high level of responsibility and I
                                        easily adapt to varying cultures and environments, giving my best at every opportunity giving to me.
                                    </p>



                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <h2>Projects</h2>
                            <div class="row">
                                @foreach (var item in Model.Projects)
                                {

                                    <div class="col-md-4">
                                        <img src="~/assets/@item.PhotoUrlsm" class="img-responsive img-rounded border box-shadow" alt="Lloyd" style="width:250px; height:350px;">
                                        <h3 class="h5">@item.ProjectName</h3>
                                        <p>@item.YearCreated</p>
                                        <a href="@item.Link">Link</a>
                                    </div>
                                }
                                









                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <h2>Work</h2>

                            @foreach (var item in Model.Experiences)
                            {

                                <div class="work-experience">
                                    <small class="date">@item.YearStarted - @item.YearEnded</small>
                                    <h3 class="h5 date-title">@item.JobTitle - <a href="#" title="">@item.Employer</a></h3>

                                    <p>@item.Description</p>
                                </div>
                            }

                            @*<div class="work-experience">
                                    <small class="date">2017-2015</small>
                                    <h3 class="h5 date-title">Web developer - <a href="http://mashup-template.com" title="">Mashup Template</a></h3>

                                    <p>Fermentum leo vel orci porta non. Posuere sollicitudin aliquam ultrices sagittis. Aliquam faucibus purus in massa tempor nec.</p>
                                </div>*@
                        </div>
                    </div>


                    <div class="card">
                        <div class="card-block">
                            <h2>Education</h2>
                            <div class="row">

                                @foreach (var item in Model.Educations)
                                {
                                    <div class="col-md-4">
                                        <div class="education-experience">
                                            <small class="date">@item.YearStarted - @item.YearEnded</small>
                                            <h3 class="h5 date-title">@item.Degree</h3>
                                            <h3 class="h5 date-title">@item.Discipline</h3>
                                            <p>@item.InstitutionName</p>
                                        </div>

                                    </div>

                                }


                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <h2>Language</h2>
                            <div class="row text-center">
                                <div class="col-md-4">
                                    <div class="language-experience">
                                        <h3 class="h5">English  <small>Bilingual</small></h3>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="language-experience">
                                        <h3 class="h5">Igbo  <small>Intermediate</small></h3>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <h2>Projects</h2>
                            <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">


                                <!-- Wrapper for slides -->
                                <div class="carousel-inner" role="listbox">

                                    @foreach (var item in Model.Projects)
                                    {
                                        @if (count == 0)
                                        {

                                            <div class="item active">
                                                @*<img src="~/assets/@item.PhotoUrlbg" class="img-responsive" alt="...">*@
                                                <img src="./assets/@item.PhotoUrlbg" class="img-responsive img-rounded" alt="...">
                                                <div class="carousel-caption">
                                                    <h3 class="h5">@item.ProjectName</h3>
                                                    <p>@item.YearCreated</p>
                                                </div>
                                            </div>
                                            count++;
                                        }
                                        else
                                        {


                                            <div class="item ">
                                                @*<img src="~/assets/@item.PhotoUrlbg" class="img-responsive" alt="...">*@
                                                <img src="./assets/@item.PhotoUrlbg" class="img-responsive" alt="...">
                                                <div class="carousel-caption">
                                                    <h3 class="h5">@item.ProjectName</h3>
                                                    <p>@item.YearCreated</p>
                                                </div>
                                            </div>
                                        }


                                    }


                                </div>

                                <!-- Indicators -->
                                <ol class="carousel-indicators">
                                    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                                    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                                    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                                </ol>

                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <h2>Social Network</h2>
                            <div class="row">
                                <div class="col-md-3">
                                    <p class="social-buttons"><a href="https://twitter.com/" title=""><span class="social-round-icon fa-icon"><i class="fa fa-twitter"></i></span>Odd_Cubes</a></p>
                                </div>
                                <div class="col-md-3">
                                    <p class="social-buttons"><a href="https://www.linkedin.com/in/eugene-lloyduche-8a12a1192/" title=""><span class="social-round-icon fa-icon"><i class="fa fa-linkedin"></i></span>Lloyd_Uche</a></p>
                                </div>
                                <div class="col-md-3">
                                    <p class="social-buttons"><a href="#" title=""><span class="social-round-icon fa-icon"><i class="fa fa-dribbble"></i></span>Lloyd Uche</a></p>
                                </div>
                                <div class="col-md-3">
                                    <p class="social-buttons"><a href="#" title=""><span class="social-round-icon fa-icon"><i class="fa fa-facebook"></i></span>Eugene LLoyd</a></p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-block">
                            <h2>Contact</h2>
                            <form class="reveal-content" method="post" asp-action="MailMe" asp-controller="Home">
                                <div class="form-group">
                                    <input type="email" class="form-control" id="email" placeholder="Email" name="email">
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control" id="subject" placeholder="Subject" name="subject">
                                </div>
                                <div class="form-group">
                                    <textarea class="form-control" rows="5" placeholder="Enter your message" name="message"></textarea>
                                </div>
                                <div class="form-group">
                                    <button type="submit" class=" btn btn-primary">Send message</button>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>
            </div>

        </div>

        @RenderBody();
    </main>
    <footer class="footer-container white-text-container text-center">
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <p><img src="./assets/images/mashup-icon.svg" alt=""></p>

                    <p>©All right reserved. Design <a href="http://www.mashup-template.com/" title="Create website with free html template">Mashup Template</a>/<a href="https://unsplash.com/" title="Beautiful Free Images">Unsplash</a></p>
                    <p>
                        <a class="fa-icon fa-icon-2x" href="https://facebook.com/" title="">
                            <i class="fa fa-facebook"></i>
                        </a>
                        <a class="fa-icon fa-icon-2x" href="https://twitter.com/" title="">
                            <i class="fa fa-twitter"></i>
                        </a>
                        <a class="fa-icon fa-icon-2x" href="https://dribbble.com/" title="">
                            <i class="fa fa-dribbble"></i>
                        </a>
                        <a class="fa-icon fa-icon-2x" href="https://www.linkedin.com/" title="">
                            <i class="fa fa-linkedin"></i>
                        </a>
                        <a class="fa-icon fa-icon-2x" href="https://vimeo.com/" title="">
                            <i class="fa fa-vimeo"></i>
                        </a>
                    </p>
                </div>
            </div>
        </div>
    </footer>

    <script>
        document.addEventListener("DOMContentLoaded", function (event) {
            scrollRevelation('.card');
        });
    </script>
